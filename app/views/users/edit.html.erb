<h1>プロフィール編集ページ</h1>
<p>テストページです</p>

<%=  link_to "アカウント", users_account_path %>
<%=  link_to "プロフィール", users_profile_path %>

<div>
  <%= form_with model: @user, url:users_profile_update_path do |f| %>
    <div>
      <%= f.label :icon ,"アイコン画像" %>
      <%= f.file_field :icon %>
    </div>
    
    <div>
      <%= f.label :name ,"名前" %>
      <%= f.text_field :name %>
    </div>

    <div>
      <%= f.label :introduction ,"自己紹介" %>
      <%= f.text_area :introduction %>
    </div>

    <div>
      <%= f.submit "保存" %>
      <%= link_to "キャンセル", users_profile_path %>
    </div>
  <% end %>
</div>
